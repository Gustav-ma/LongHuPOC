@using ModelResources = LongHu.Framework.Resources.ProjectPlanCollectionResources
@using GlobalResources = Resources.Global
@model IEnumerable<LongHu.ObjectModel.ProjectPlanCollection>


<div class="gridViewOverflow">
<table class="grid" cellpadding="0" cellspacing="0">
    <tr class="header"  align="left">
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Year">@ModelResources.LabelYear</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="AgreementNumber">@ModelResources.LabelAgreementNumber</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Unit">@ModelResources.LabelUnit</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Number">@ModelResources.LabelNumber</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="UnitPrice">@ModelResources.LabelUnitPrice</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="EstimateValue">@ModelResources.LabelEstimateValue</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="QualityRequirement">@ModelResources.LabelQualityRequirement</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="ReviewTime">@ModelResources.LabelReviewTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="DrawingProvidedTime">@ModelResources.LabelDrawingProvidedTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="ContractTime">@ModelResources.LabelContractTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="BidStartedTime">@ModelResources.LabelBidStartedTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="BidSeningTime">@ModelResources.LabelBidSeningTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="BidReturnTime">@ModelResources.LabelBidReturnTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="BidCheckTime">@ModelResources.LabelBidCheckTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="ContractSignTime">@ModelResources.LabelContractSignTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="ComingIntoTime">@ModelResources.LabelComingIntoTime</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Comments">@ModelResources.LabelComments</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="CreatedOn">@ModelResources.LabelCreatedOn</th>
       

                 <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="ModifiedOn">@ModelResources.LabelModifiedOn</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="ProjectPlan">@ModelResources.LabelProjectPlanId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Contract">@ModelResources.LabelContractId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="OrgChart">@ModelResources.LabelOrgChartId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="DataDictionary">@ModelResources.LabelBidTypeId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Department">@ModelResources.LabelDepartmentId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Employee">@ModelResources.LabelResearchOwnerEmployeeId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Employee">@ModelResources.LabelEngeerEmployeeId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Employee">@ModelResources.LabelCostOwnerEmployeeId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Employee">@ModelResources.LabelAuthorEmployeeId</th>
       

               
			   <th  align="left" scope="col" class="grid_header_th" mth="Sort" IsAsc="@ViewBag.IsAsc.ToString()" SortBy="Employee">@ModelResources.LabelOrganizerEmployeeId</th>
        <th>@GlobalResources.LabelAction</th>
    </tr>

@foreach (var item in Model) {
    <tr class="row" valign="top" id="row-@item.Id">
                 <td>@Html.DisplayFor(modelItem => item.Year)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.AgreementNumber)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.Unit)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.Number)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.UnitPrice)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.EstimateValue)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.QualityRequirement)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.ReviewTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.DrawingProvidedTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.ContractTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.BidStartedTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.BidSeningTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.BidReturnTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.BidCheckTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.ContractSignTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.ComingIntoTime)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.Comments)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.CreatedOn)&nbsp;</td>
                 <td>@Html.DisplayFor(modelItem => item.ModifiedOn)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.ProjectPlan.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.Contract.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.OrgChart.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.DataDictionary.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.Department.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.Employee.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.Employee.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.Employee.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.Employee.Name)&nbsp;</td>
               <td>@Html.DisplayFor(modelItem => item.Employee.Name)&nbsp;</td>
        <td class="command">
            @Html.ActionLink(GlobalResources.ListActionEdit, "Edit", new { id=item.Id }) |
            @Html.ActionLink(GlobalResources.ListActionDetails, "Details", new { id=item.Id }) |
        
			<a href="#" class="deleteLink" data-id="@item.Id" data-delConfirmMessage="@ModelResources.DeleteConfirmMessage">@GlobalResources.ListActionDelete</a>
        </td>

    </tr>
}

</table>
</div>
<div id="Page">@Html.Partial("_Page", Model as IPagedList)</div>

